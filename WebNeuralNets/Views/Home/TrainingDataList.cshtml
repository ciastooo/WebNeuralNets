@model WebNeuralNets.Models.Dto.LoginModelDto

@{
    Layout = "~/Views/_layout.cshtml";
}

<div class="header">
    <h1 class="translate">MENU_NEURALNETSLIST</h1>
</div>
<div class="tableContent">
    <table class="table">
        <thead>
            <tr>
                <th width="5%">Id</th>
                <th width="20%" class="translate">TEXT_NAME</th>
                <th width="15%" class="translate">TEXT_INPUT</th>
                <th width="15%" class="translate">TEXT_INPUT</th>
                <th width="10%" class="translate">TEXT_OUTPUT</th>
                <th width="35%"></th>
            </tr>
        </thead>
        <tbody id="tableBody"></tbody>
    </table>
    <div class="col-md-12">
        <div class="form-group col-md-3">
            <input id="Name" placeholder="Name" class="form-control" />
            <span id="NameValidation" class="text-danger hidden translate">VALIDATION_FIELDREQUIRED</span>
        </div>
        <div class="form-group col-md-3">
            <input id="Input1" placeholder="Input" class="form-control" type="number" step="0.1" />
            <span id="Input1Validation" class="text-danger hidden translate">VALIDATION_FIELDREQUIRED</span>
        </div>
        <div class="form-group col-md-3">
            <input id="Input2" placeholder="Input" class="form-control" type="number" step="0.1" />
            <span id="Input2Validation" class="text-danger hidden translate">VALIDATION_FIELDREQUIRED</span>
        </div>
        <div class="form-group col-md-2">
            <input id="Output" placeholder="Output" class="form-control" type="number" step="0.1" />
            <span id="OutputValidation" class="text-danger hidden translate">VALIDATION_FIELDREQUIRED</span>
        </div>
        <div class="form-gorup col-md-1">
            <button id="addButton" class="btn btn-primary pull-right translate">TEXT_ADD</button>
        </div>
    </div>
</div>


<script src="~/js/trainingDataList.js"></script>
